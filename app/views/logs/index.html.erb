<h1>Logs</h1>

<table>
  <thead>
    <tr>
      <th>IP Address</th>
      <th>Created</th>
    </tr>
  </thead>
  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.ip_address %></td>
        <td><%= log.created_at %></td>
        <td><%= link_to "View", log_path(log) %></td>
        <td><%= link_to "Delete", log_path(log), :method => :delete, :confirm => "Are you sure you want to delete this log?" %>
        <td><%= link_to "Edit", edit_log_path(log) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
